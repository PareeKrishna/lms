{
  "version": 2,
  "builds": [
    {
      "src": "api/stripe.js",
      "use": "@vercel/node"
    },
    {
      "src": "server.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["dist/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/api/stripe",
      "dest": "api/stripe.js"
    },
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "functions": {
    "api/stripe.js": {
      "memory": 1024,
      "maxDuration": 10
    },
    "server.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  }
}
